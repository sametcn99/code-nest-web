{
	"trailingComma": "es5",
	"singleAttributePerLine": true,
	"semi": false,
	"singleQuote": true,
	"plugins": ["prettier-plugin-tailwindcss"],
	"jsxSingleQuote": true,
	"useTabs": true,
	"quoteProps": "as-needed",
	"tabWidth": 1,
	"bracketSpacing": true,
	"endOfLine": "auto",
	"arrowParens": "always",
	"embeddedLanguageFormatting": "auto"
}
